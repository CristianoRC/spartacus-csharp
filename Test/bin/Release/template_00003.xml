    <command>
        <sql>
            select to_char(t.inst_in_codigo) as inst_in_codigo,
                   to_char(t.agente) as agente,
                   to_char(t.agn_st_nome) as agn_st_nome,
                   to_char(t.nota) as nota,
                   to_char(t.ent_st_serie) as ent_st_serie,
                   to_char(t.data, 'dd/mm/yyyy') as data,
                   to_char(t.cfop) as cfop,
                   to_char(t.ent_st_di) as ent_st_di,
                   to_char(t.produto) as produto,
                   'Produto: ' || to_char(t.produto) || ' - ' || to_char(t.pro_st_descricao) as pro_st_descricao,
                   to_char(t.pro_st_unidade) as pro_st_unidade,
                   to_char(t.pro_st_ncm) as pro_st_ncm,
                   to_char(t.ent_re_quantidade) as ent_re_quantidade,
                   to_char(t.ent_re_custofob) as ent_re_custofob,
                   to_char(t.ent_re_ii) as ent_re_ii,
                   to_char(t.ent_re_frete) as ent_re_frete,
                   to_char(t.ent_re_seguro) as ent_re_seguro,
                   to_char(t.ent_re_aduaneiras) as ent_re_aduaneiras,
                   to_char(t.ent_re_complementares) as ent_re_complementares,
                   to_char(t.ent_re_custocif) as ent_re_custocif,
                   to_char(t.ent_re_pis) as ent_re_pis,
                   to_char(t.ent_re_cofins) as ent_re_cofins,
                   to_char(t.ent_re_icms) as ent_re_icms,
                   to_char(t.ent_re_ipi) as ent_re_ipi,
                   to_char(t.grupo) as grupo,
                   to_char(t.paraisofiscal) as paraisofiscal,
                   to_char(t.cfop_bo_calculo) as cfop_bo_calculo,
                   to_char(gp.produto_custofob) as produto_custofob,
                   to_char(gp.produto_ii) as produto_ii,
                   to_char(gp.produto_frete) as produto_frete,
                   to_char(gp.produto_seguro) as produto_seguro,
                   to_char(gp.produto_aduaneiras) as produto_aduaneiras,
                   to_char(gp.produto_complementares) as produto_complementares,
                   to_char(gp.produto_custocif) as produto_custocif,
                   to_char(gg.geral_custofob) as geral_custofob,
                   to_char(gg.geral_ii) as geral_ii,
                   to_char(gg.geral_frete) as geral_frete,
                   to_char(gg.geral_seguro) as geral_seguro,
                   to_char(gg.geral_aduaneiras) as geral_aduaneiras,
                   to_char(gg.geral_complementares) as geral_complementares,
                   to_char(gg.geral_custocif) as geral_custocif,
                   #CABECALHO#
            from tab00003 t,
            (
            select produto,
                   sum(ent_re_custofob) as produto_custofob,
                   sum(ent_re_ii) as produto_ii,
                   sum(ent_re_frete) as produto_frete,
                   sum(ent_re_seguro) as produto_seguro,
                   sum(ent_re_aduaneiras) as produto_aduaneiras,
                   sum(ent_re_complementares) as produto_complementares,
                   sum(ent_re_custocif) as produto_custocif
            from tab00003
            where 1 = 1
              #FILTRO#
            group by produto
            ) gp,
            (
            select sum(ent_re_custofob) as geral_custofob,
                   sum(ent_re_ii) as geral_ii,
                   sum(ent_re_frete) as geral_frete,
                   sum(ent_re_seguro) as geral_seguro,
                   sum(ent_re_aduaneiras) as geral_aduaneiras,
                   sum(ent_re_complementares) as geral_complementares,
                   sum(ent_re_custocif) as geral_custocif
            from tab00003
            where 1 = 1
              #FILTRO#
            ) gg
            where t.produto = gp.produto
              #FILTRO#
        </sql>
        <parameters>
            <parameter>
                <name>CABECALHO</name>
                <type>UNDEFINED</type>
            </parameter>
            <parameter>
                <name>FILTRO</name>
                <type>UNDEFINED</type>
            </parameter>
        </parameters>
    </command>

